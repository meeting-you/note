{
  "author": "萝卜馋青菜 <fast_ant@163.com>",
  "description": "学习小笔记",
  "generator": "site",
  "language": "zh-hans",
  "links": {
    "sidebar": {
      "我的笔记": "https://runing-time.github.io/gitbook_test/"
    }
  },
  "pdf": {
    "fontSize": 12,
    "footerTemplate": null,
    "headerTemplate": null,
    "margin": {
      "bottom": 36,
      "left": 62,
      "right": 62,
      "top": 36
    },
    "pageNumbers": false,
    "paperSize": "a4"
  },
  "plugins": [
    "theme-fexa",
    "tbfed-pagefooter",
    "hide-element",
    "expandable-chapters",
    "chapter-fold",
    "-lunr", "-search", "search-pro",
    "prism", "-highlight",
    "splitter",
    "anchor-navigation-ex",
    "pageview-count"
  ],
  "variables": {
    "themeFexa":{
      "nav":[
        {
          "url":"https://runing-time.github.io/gitbook_test",
          "target":"_blank",
          "name": "学习笔记"
        }
      ]
    }
  },
  "pluginsConfig": {
    "anchor-navigation-ex": {
      "showLevel": true,
      "showGoTop": true
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-okaidia.css"
      ]
    },
    "tbfed-pagefooter": {
      "copyright":"",
      "modify_label": "该文件最后修改时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "hide-element": {
      "elements": [".gitbook-link"]
    },
    "disqus": {
      "shortName": "shiguang"
    },
    "theme-fexa":{
      "search-placeholder":"输入关键字搜索",
      "logo":"./logo.png",
      "favicon": "./favicon.ico"
    }
  },
  "title": "时光 笔记"
}